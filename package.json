  "version": "4.0.0",
  "description": "Programma ufficiale italiano per la gestione delle gare di robotica",
  "main": "index.js",
  "scripts": {
    "start": "node index",
    "reset": "node index -r",
    "debug": "node index -d",
    "watch": "nodemon index",
