  },
  "homepage": "https://github.com/lusvelt/robocupnetwork#readme",
  "dependencies": {
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "fs-extra": "^7.0.1",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10",
    "make-runnable": "^1.3.6",
